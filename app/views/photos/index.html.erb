<% if current_admin%>
	<%=link_to 'create new', photos_new_path %>
<%end%>
<br><br><Br>
<div class="photo-gallery">
	<%@photos.each do |photo| %>
		<%url = photo.file_data.url%>

		<div class="gallery-image">
			<img src = <%="#{url}"%> class="gallery-image"/>
			<ul>
				<% if current_admin %>
			<%= link_to "delete",photo_path(photo), :method=>:delete%>
			<%=link_to "edit", edit_photo_path(photo), :method => "get"%>
			<%end%>
			</ul>
		</div>
	<%end%>
</div>